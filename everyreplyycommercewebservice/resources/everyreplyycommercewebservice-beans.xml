<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<enum class="com.everyreplyycommercewebservice.populator.options.PaymentInfoOption">
		<value>BASIC</value>
		<value>BILLING_ADDRESS</value>
	</enum>

	<!--auth response  -->
	<bean class="com.everyreplyycommercewebservice.auth.data.LogoutResponse">
		<property name="success" type="boolean"/>
	</bean>

	<!-- storesession data -->

	<bean class="com.everyreplyycommercewebservice.storesession.data.LanguageDataList">
		<property name="languages"
		          type="java.util.Collection&lt;de.hybris.platform.commercefacades.storesession.data.LanguageData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.storesession.data.CurrencyDataList">
		<property name="currencies"
		          type="java.util.Collection&lt;de.hybris.platform.commercefacades.storesession.data.CurrencyData>"/>
	</bean>

	<!-- order data -->

	<bean class="com.everyreplyycommercewebservice.order.data.CartDataList">
		<property name="carts"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.CartData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.order.data.OrderEntryDataList">
		<property name="orderEntries"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.OrderEntryData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.order.data.CardTypeDataList">
		<property name="cardTypes"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.CardTypeData>"/>
	</bean>

	<bean class="de.hybris.platform.commercefacades.order.data.CartModificationDataList">
		<property name="cartModificationList"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.CartModificationData>"/>
	</bean>

	<!-- product data -->

	<bean class="com.everyreplyycommercewebservice.product.data.ProductDataList">
		<property name="products"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.ProductData>"/>
		<property name="catalog" type="String"/>
		<property name="version" type="String"/>
		<property name="totalProductCount" type="int"/>
		<property name="totalPageCount" type="int"/>
		<property name="currentPage" type="int"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.product.data.PromotionDataList">
		<property name="promotions"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.PromotionData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.product.data.PromotionResultDataList">
		<property name="promotions"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.PromotionResultData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.product.data.ReviewDataList">
		<property name="reviews" type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.ReviewData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.product.data.SuggestionDataList">
		<property name="suggestions"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.SuggestionData>"/>
	</bean>

	<!-- user data -->

	<bean class="com.everyreplyycommercewebservice.user.data.TitleDataList">
		<property name="titles" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.TitleData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.user.data.CountryDataList">
		<property name="countries" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.CountryData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.user.data.AddressDataList">
		<property name="addresses" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.AddressData>"/>
	</bean>

	<!-- voucher data -->
	<bean class="com.everyreplyycommercewebservice.voucher.data.VoucherDataList">
		<property name="vouchers"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.voucher.data.VoucherData>"/>
	</bean>

	<!-- validation data -->
	<bean class="com.everyreplyycommercewebservice.validation.data.AddressValidationData">
		<property name="errors" type="de.hybris.platform.webservicescommons.dto.error.ErrorListWsDTO"/>
		<property name="decision" type="String"/>
		<property name="suggestedAddressesList"
		          type="com.everyreplyycommercewebservice.user.data.AddressDataList"/>
	</bean>

	<!-- Additional title property for CustomerData -->
	<bean class="de.hybris.platform.commercefacades.user.data.CustomerData"
	      extends="de.hybris.platform.commercefacades.user.data.PrincipalData">
		<property name="title" type="String"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.user.data.LoginChangeResponse">
		<property name="success" type="boolean"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.user.data.PasswordRestoreResponse">
		<property name="success" type="boolean"/>
	</bean>

	<!-- update queues -->

	<bean class="com.everyreplyycommercewebservice.queues.data.ProductExpressUpdateElementData">
		<property name="code" type="String"/>
		<property name="catalogId" type="String"/>
		<property name="catalogVersion" type="String"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.queues.data.ProductExpressUpdateElementDataList">
		<property name="productExpressUpdateElements"
		          type="java.util.List&lt;com.everyreplyycommercewebservice.queues.data.ProductExpressUpdateElementData>"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.queues.data.OrderStatusUpdateElementData">
		<property name="code" type="String"/>
		<property name="status" type="String"/>
		<property name="baseSiteId" type="String"/>
	</bean>

	<bean class="com.everyreplyycommercewebservice.queues.data.OrderStatusUpdateElementDataList">
		<property name="orderStatusUpdateElements"
		          type="java.util.List&lt;com.everyreplyycommercewebservice.queues.data.OrderStatusUpdateElementData>"/>
	</bean>

</beans>
